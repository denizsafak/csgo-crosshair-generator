{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "import/prefer-default-export": 0,
        "react/sort-comp": 0,
        "jsx-a11y/no-static-element-interactions": 0
    }
}
